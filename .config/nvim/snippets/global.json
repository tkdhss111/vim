{
  "YAML/Presentation": 
  { "prefix": "yaml",
    "body":
    [
      "---",
      "title: '$1'",
      "subtitle: '$2'",
      "format:",
      "\trevealjs:",
      "\tauthor: 'Hisashi Takeda, Ph.D.'",
      "\ttoc: true",
      "\tslide-number: true",
      "\tnumber-sections: true",
      "\tchalkboard: true",
      "\tincremental: false",
      "\tlightbox: true",
      "\tcode-line-numbers: true",
      "\tcode-fold: true",
      "\tcode-copy: true",
      "\tsmaller: false ",
      "\tscrollable: true",
      "\tlang: 'ja'",
      "\ttheme: [default, ../../../css/slide.scss]",
      "\tlogo: ../../../css/takenoko.png",
      "execute:",
      "\tcache: true",
      "\tfreeze: auto",
      "\t\tembed-resources: false",
      "---\n"
    ]
  },

  "YAML/Footnote": 
  { "prefix": "footnote",
    "body":
    [
      "[^$1]",
      "[^$1]: ",
    ]
  },

  "Basic":        { "prefix": "ba", "body": "T1=$1 T2=$2 T0=$0"         },
  "Placeholders": { "prefix": "pl", "body": "T1=${1:aa}\nT2=${2:<$1>}"  },
  "Choices":      { "prefix": "ch", "body": "T1=${1|a,b|} T2=${2|c,d|}" },
  "Linked":       { "prefix": "li", "body": "T1=$1\n\tT1=$1"            },
  "Variables":    { "prefix": "va", "body": "Runtime: $VIMRUNTIME\n"    },
  "Complex":      {
    "prefix": "co",
    "body": [ "T1=${1:$RANDOM}", "T3=${3:$1_${2:$1}}", "T2=$2" ]
  }
}
